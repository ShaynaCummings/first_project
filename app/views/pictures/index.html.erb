<div>
  <h3>library of all gifs</h3>
</div>

  <ul>
      <% @pictures.each do |picture| %>
        <% image = "<img src=\"#{picture.url}\">" %>
          <li class="img-thumbnail thumbnail-caption">
            <a href="<%= picture_path(picture.user.id) %>"><img class= "thumbs-lib" src="<%= picture.url %>"></a><br />
            by: <%= link_to picture.user.name, user_path(picture.user) %>
          </li>
      <% end %>
  </ul>



