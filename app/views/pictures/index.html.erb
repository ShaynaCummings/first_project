<div>
  <h3>library of all gifs</h3>
</div>

<p>
<%= link_to "Add a new gif", new_picture_path %>
</p>

  <table class="table">
    <tbody>
      <% @pictures.each do |picture| %>
        <% image = "<img src=\"#{picture.url}\">" %>
          <td>
            from: <%= picture.user.email %>
            <a href="<%= picture_path(picture.user.id) %>"><img src="<%= picture.url %>" class="img-thumbnail" width= 100px></a>
          </td>

      <% end %>
    </tbody>
  </table>



